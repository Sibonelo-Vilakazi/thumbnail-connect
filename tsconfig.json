{
    "compilerOptions": {
      "target": "esnext",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "isolatedModules": true,
      "jsx": "react-jsx",
      "declaration": false,
      "outDir": "./build/types",
      "esModuleInterop": true
    },
    "include": [
      "app"
    ],
    "extends": "expo/tsconfig.base"
  }
  